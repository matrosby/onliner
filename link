function makeInfoPopupSemiTransparent(){setCSS(`\n\t\t.info.popup {\n\t\t\tbackground:rgba(0,0,0,0.7);\n\t\t}\n\t`)}
function moveRefsSortButton(){setCSS(`\n\t\t.sbgcui_refs-sort-button {\n\t\t\tbottom: 120px;\n\t\t\tright: 55px;\n\t\t}\n\t`)}
function makePointInfoButton(){setCSS(`\n\t\t.i-buttons>button {\n\t\t\tmin-height: 60px;\n\t\t}\n\t`)}
function disableHitTolerance(data){return data.replace(/const HIT_TOLERANCE = \d+/,"const HIT_TOLERANCE = 0")}
